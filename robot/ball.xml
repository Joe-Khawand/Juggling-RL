<mujoco model="balls">
    <!--To change the bouncing off the balls change the solref parameter-->
    <worldbody>
        <body name="Ball1" pos="1 0.5 1">
        <freejoint/>
        <geom name="ball1" type="sphere" size=".02" solref="-1000 0" rgba="1 0 0 1"/>
        </body>
        <body name="Ball2" pos="1 -0.5 1">
        <freejoint/>
        <geom name="ball2" type="sphere" size=".02" solref="-1000 0" rgba="0 1 0 1"/>
        </body>
    </worldbody>
</mujoco>
